{
  "manifest_version": 2,
  "name": "Garmin Connect Extender",
  "description": "",
  "version": "1.0.6",
  "homepage_url": "https://github.com/phlexo/garmin-connect-extender/",
  "icons": {
    "32": "icons/gce-32.png",
    "48": "icons/gce-48.png",
    "64": "icons/gce-64.png",
    "128": "icons/gce-128.png"
  },
  "background": {
    "scripts": ["utility/jquery-3.2.1.min.js", "dashboard/background.js"]
  },
  "content_scripts": [{
    "matches": ["*://connect.garmin.com/modern*"],
    "js": ["utility/jquery-3.2.1.min.js", "utility/tmpl.min.js", "fullscreen-maps/content.js", "dashboard/content.js"]
  }],
  "permissions": [
    "https://connect.garmin.com/modern*",
    "tabs",
    "webRequest",
    "storage"
  ],
  "applications": {
    "gecko": {
      "id": "garmin-connect-extender@phlexo.se",
      "strict_min_version": "52.0"
    }
  }
}